[uwsgi]
chdir = /opt/mozdef/envs/mozdef/mq/
uid = mozdef
mule = esworker.py
pyargv = -c /opt/mozdef/envs/mozdef/mq/esworker.fxastage.conf
daemonize = /opt/mozdef/envs/mozdef/logs/uwsgi.mqESmules.fxastage.log
;ignore normal operations that generate nothing but normal response
;
log-drain = generated 0 bytes
log-date = %%a %%b %%d %%H:%%M:%%S
socket = /opt/mozdef/envs/mozdef/mq/mqESmulesFxaStage.socket
virtualenv = /opt/mozdef/envs/mozdef/
master-fifo = /opt/mozdef/envs/mozdef/mq/mqESmulesFxaStage.fifo
never-swap
pidfile= /opt/mozdef/envs/mozdef/mq/mqESmulesFxaStage.pid
vacuum = true
enable-threads
